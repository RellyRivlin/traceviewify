{
  "name": "traceviewify",
  "version": "0.4.0",
  "description": "Converts .cpuprofile format to trace viewer JSON object format to allow analysing the data in chrome://tracing.",
  "main": "index.js",
  "bin": "bin/traceviewify",
  "scripts": {
    "test": "tape test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/traceviewify.git"
  },
  "homepage": "https://github.com/thlorenz/traceviewify",
  "dependencies": {},
  "devDependencies": {
    "tape": "^3.5.0"
  },
  "keywords": [
    "trace-viewer",
    "cpuprofile",
    "converter",
    "debug",
    "profile",
    "trace"
  ],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/traceviewify/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=0.8"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest"
    ]
  }
}
