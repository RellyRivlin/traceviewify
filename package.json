{
    "name": "traceviewify",
    "version": "0.0.0",
    "description": "Converts .cpuprofile format to trace viewer JSON object format to allow analysing the data in chrome://tracing.",
    "main": "index.js",
    "scripts": {
        "test-main": "tap test/*.js",
        "test-0.8": "nave use 0.8 npm run test-main",
        "test-0.10": "nave use 0.10 npm run test-main",
        "test-all": "npm run test-main && npm run test-0.8 && npm run test-0.10",
        "test": "if [ -e $TRAVIS ]; then npm run test-all; else npm run test-main; fi"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/thlorenz/traceviewify.git"
    },
    "homepage": "https://github.com/thlorenz/traceviewify",
    "dependencies": {},
    "devDependencies": {
        "nave": "~0.4.3",
        "tap": "~0.4.3"
    },
    "keywords": [],
    "author": {
        "name": "Thorsten Lorenz",
        "email": "thlorenz@gmx.de",
        "url": "http://thlorenz.com"
    },
    "license": {
        "type": "MIT",
        "url": "https://github.com/thlorenz/traceviewify/blob/master/LICENSE"
    },
    "engine": {
        "node": ">=0.8"
    },
    "testling": {
        "files": "test/*.js",
        "browsers": [
            "ie/8..latest",
            "firefox/17..latest",
            "firefox/nightly",
            "chrome/22..latest",
            "chrome/canary",
            "opera/12..latest",
            "opera/next",
            "safari/5.1..latest",
            "ipad/6.0..latest",
            "iphone/6.0..latest"
        ]
    }
}
